{
  "name": "fastify-vercel",
  "version": "1.0.0",
  "description": "Fastify vercel serverless starter template",
  "author": "Ashish Kumar",
  "main": "api/server.js",
  "scripts": {
    "dev": "tsx -r dotenv/config api/dev-server.ts dotenv_config_path=.env.development dotenv_config_debug=true",
    "start": "vercel dev",
    "deploy:dev": "vercel deploy",
    "deploy:prod": "vercel deploy --prod"
  },
  "keywords": [
    "fastify",
    "vercel",
    "node",
    "serverless"
  ],
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "fastify": "^4.25.2",
    "tslib": "^2.6.2",
    "tsx": "^4.7.0",
    "vercel": "^33.0.1"
  }
}
